{
    "name": "neuraldeck",
    "version": "0.4.5",
    "description": "NeuralDeck - AI Provider Manager - Access ChatGPT, Gemini, Claude and more from a floating interface",
    "type": "module",
    "main": "dist-electron/main.cjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/murapadev/NeuralDeck.git"
    },
    "homepage": "https://github.com/murapadev/NeuralDeck#readme",
    "bugs": {
        "url": "https://github.com/murapadev/NeuralDeck/issues"
    },
    "author": {
        "name": "Pablo RM",
        "email": "pablo@example.com"
    },
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build && electron-builder",
        "build:win": "tsc && vite build && electron-builder --win",
        "build:linux": "tsc && vite build && electron-builder --linux",
        "build:mac": "tsc && vite build && electron-builder --mac",
        "preview": "vite preview",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 50",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "prepare": "husky install"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.1.0",
        "@dnd-kit/sortable": "^8.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@radix-ui/react-alert-dialog": "^1.0.0",
        "@radix-ui/react-avatar": "^1.0.0",
        "@radix-ui/react-checkbox": "^1.0.0",
        "@radix-ui/react-dialog": "^1.0.0",
        "@radix-ui/react-label": "^1.0.0",
        "@radix-ui/react-popover": "^1.0.0",
        "@radix-ui/react-progress": "^1.0.0",
        "@radix-ui/react-scroll-area": "^1.0.0",
        "@radix-ui/react-select": "^1.0.0",
        "@radix-ui/react-separator": "^1.0.0",
        "@radix-ui/react-slider": "^1.0.0",
        "@radix-ui/react-slot": "^1.0.0",
        "@radix-ui/react-switch": "^1.0.0",
        "@radix-ui/react-tabs": "^1.0.0",
        "@radix-ui/react-tooltip": "^1.0.0",
        "@tanstack/react-query": "^4.36.1",
        "@trpc/client": "^10.45.2",
        "@trpc/react-query": "^10.45.2",
        "@trpc/server": "^10.45.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "electron-log": "^5.1.7",
        "electron-store": "^8.2.0",
        "electron-trpc": "^0.7.1",
        "electron-updater": "^6.2.0",
        "lucide-react": "^0.452.0",
        "react-hook-form": "^7.53.2",
        "react-markdown": "^9.0.1",
        "remark-gfm": "^4.0.0",
        "sonner": "^1.5.0",
        "tailwind-merge": "^2.2.0",
        "zod": "^3.23.8",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@playwright/test": "^1.58.2",
        "@testing-library/react": "^14.1.2",
        "@types/node": "^20.10.4",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/sharp": "^0.32.0",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/ui": "4.0.18",
        "autoprefixer": "^10.4.24",
        "electron": "39.2.7",
        "electron-builder": "^26.7.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^9.0.0",
        "jsdom": "^27.4.0",
        "lint-staged": "^15.2.0",
        "postcss": "^8.4.32",
        "prettier": "^3.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sharp": "^0.34.5",
        "tailwindcss": "^3.3.6",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.3.3",
        "typescript-eslint": "^7.18.0",
        "vite": "7.3.1",
        "vite-plugin-electron": "^0.29.0",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vitest": "4.0.18"
    },
    "build": {
        "appId": "com.neuraldeck.app",
        "productName": "NeuralDeck",
        "directories": {
            "output": "release"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "murapadev",
                "repo": "NeuralDeck"
            }
        ],
        "files": [
            "dist/**/*",
            "dist-electron/**/*"
        ],
        "mac": {
            "target": [
                "dmg",
                "zip"
            ],
            "icon": "public/icon.png",
            "category": "public.app-category.productivity",
            "hardenedRuntime": true,
            "entitlements": "build/entitlements.mac.plist",
            "entitlementsInherit": "build/entitlements.mac.plist"
        },
        "win": {
            "target": [
                "nsis",
                "portable"
            ],
            "icon": "public/icon.png"
        },
        "linux": {
            "target": [
                "AppImage",
                "deb"
            ],
            "icon": "public/icon.png",
            "category": "Utility"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    }
}
